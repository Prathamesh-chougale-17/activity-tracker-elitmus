{
  "name": "activity-tracker",
  "displayName": "Activity tracker",
  "version": "0.0.1",
  "description": "This tracker is made for tracking the usage of browser",
  "author": "prathamesh chougale",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "bip39": "^3.1.0",
    "chart.js": "^4.4.2",
    "perf_hooks": "^0.0.1",
    "plasmo": "0.86.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "storage",
      "tabs",
      "webNavigation",
      "webRequest",
      "webRequestBlocking",
      "activeTab",
      "alarms",
      "cookies",
      "history",
      "identity"
    ]
  }
}
